<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
      <class name="com.weibao.entity.DengluUser" table="t_DlUser">

          <id name="user_id" column="user_id">
      <generator class="native"/>
      </id>

     
      <property name="userName" column="userName"/>
      <property name="telephone" column="telephone"/>
      <property name="ruzhiDate" column="ruzhiDate"/>
      <property name="userEmail" column="userEmail"/>
      <property name="xueLi" column="xueLi"/>
      <property name="userTeam" column="userTeam"/>
      <property name="password" column="password"/>
      <property name="genarl" column="genarl"/>

     
      
      </class>
    </hibernate-mapping>